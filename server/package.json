{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "engines": {
    "node": "16.20.1"
  },
  "scripts": {
    "build:server": "rm -rf dist && babel src --out-dir dist --copy-files",
    "build:client": "cd ../client && rm -rf build/ && npm install && npm run build",
    "build": "npm run build:server",
    "dev": "NODE_ENV=development nodemon --exec babel-node ./src/index.js",
    "start": "NODE_ENV=production node ./dist/index.js",
    "format:client": "cd ../client && npm run format",
    "format:server": "prettier --write \"src/**/*.js\"",
    "format": "npm run format:client && cd ../server && npm run format:server",
    "lint:client": "cd ../client && npm run linting -- --fix",
    "linting:server": "eslint --ignore-path .eslintignore './src/**/*.{ts,js}'",
    "lint": "npm run lint:client && cd ../server && npm run linting:server -- --fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.6",
    "axios": "^1.4.0",
    "bcrypt": "^5.0.1",
    "connect-redis": "^7.1.0",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "http-errors": "~1.6.3",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.3.1",
    "morgan": "~1.9.1",
    "passport": "^0.5.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^4.6.7",
    "sanitize": "^2.1.2",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.22.5",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.22.5",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "nodemon": "^1.14.9",
    "prettier": "^2.8.8"
  }
}
