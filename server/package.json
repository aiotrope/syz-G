{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "scripts": {
    "babel-node": "babel-node --presets='@babel/preset-env'",
    "dev": "NODE_ENV=development nodemon --exec yarn run babel-node -- src/index.js",
    "build:server": "rimraf dist/ && babel src --out-dir dist --copy-files",
    "start": "NODE_ENV=production node ./dist/index.js",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(js|jsx|json|md|ts|tsx|css|scss)\"",
    "linting": "eslint --ignore-path .eslintignore './**/*.{ts,mjs,js}'",
    "lint": "yarn run linting -- --fix",
    "test": "NODE_ENV=test jest --verbose --runInBand --forceExit --detectOpenHandles",
    "start:test": "NODE_ENV=test node ./dist/index.js"
  },
  "dependencies": {
    "@babel/register": "^7.22.5",
    "@babel/runtime": "^7.22.6",
    "bcrypt": "^5.1.0",
    "cloudinary": "^1.37.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "helmet": "^7.0.0",
    "http-errors": "~1.6.3",
    "ioredis": "^5.3.2",
    "isomorphic-dompurify": "^1.8.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "mongoose": "6.10.0",
    "morgan": "~1.9.1",
    "nocache": "^4.0.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "safe-regex": "^2.1.1",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.6",
    "@babel/core": "^7.22.8",
    "@babel/node": "^7.22.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.22.7",
    "@babel/preset-env": "^7.22.7",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3"
  }
}
