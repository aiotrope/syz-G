{
  "name": "xzymous",
  "version": "1.0.0",
  "description": "CT30A3204 capstone project.",
  "main": "./dist/index.js",
  "scripts": {
    "install:server": "cd server && npm install",
    "install:client:": "cd client && npm install",
    "build:client": "cd client && rm -rf build && npm run build",
    "build:server": "cd server && npm run build",
    "build:fullstack": "npm run build:client && npm run build:server",
    "format:client": "cd client && npm run format",
    "format:server": "cd server && npm run format",
    "lint:client": "cd client && npm run linting -- --fix",
    "lint:server": "cd server && npm run linting -- --fix",
    "dev:server": "NODE_ENV=development cd server && npm run dev",
    "dev:client": "NODE_ENV=development cd client && npm run start",
    "start": "NODE_ENV=production cd server && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aiotrope/xzymous.git"
  },
  "author": "Arnel Imperial",
  "license": "0BSD",
  "bugs": {
    "url": "https://github.com/aiotrope/xzymous/issues"
  },
  "homepage": "https://github.com/aiotrope/xzymous#readme",
  "engines": {
    "node": "16.20.1",
    "npm": "9.7.2"
  }
}